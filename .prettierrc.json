{
  "bracketSpacing": true,
  "proseWrap": "always",
  "semi": false,
  "singleQuote": true,
  "printWidth": 160,
  "trailingComma": "all",
  "arrowParens": "always",
  "importOrder": [
    "^react",
    "<THIRD_PARTY_MODULES>",
    "^@",
    "",
    "^./",
    "^../",
    "^components",
    "^containers",
    "^features",
    "^pages",
    "",
    "^env",
    "^hooks",
    "^types",
    "^((utils)|(store)|(services)|(src)|\b)"
  ],
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "importOrderCaseInsensitive": true
}
